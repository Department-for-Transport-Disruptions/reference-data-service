{
    "name": "reference-data-service",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "aws s3 cp s3://cdd-sandbox-dev-data/.env ./.env.local && sst dev",
        "build": "sst build",
        "deploy": "sst deploy",
        "remove": "sst remove",
        "console": "sst console",
        "diff": "sst diff",
        "sst": "sst",
        "cdk": "cdk",
        "typecheck": "tsc --noEmit",
        "lint": "eslint .",
        "prepare": "husky install",
        "test:uploaders": "npm -w @reference-data-service/ref-data-uploaders run test",
        "test:api": "npm -w @reference-data-service/api run test",
        "test": "npm run test:uploaders -- run & npm run test:api -- run",
        "tsc:build": "tsc --noemit"
    },
    "devDependencies": {
        "@aws-sdk/types": "^3.418.0",
        "@smithy/types": "^2.3.4",
        "@tsconfig/node18": "^1.0.1",
        "@types/aws-lambda": "^8.10.110",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "aws-cdk": "2.110.1",
        "aws-cdk-lib": "2.110.1",
        "@aws-sdk/types": "^3.418.0",
        "aws-sdk-client-mock": "^3.0.0",
        "constructs": "10.3.0",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.4",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vitest": "^0.0.48",
        "husky": "^8.0.0",
        "mockdate": "^3.0.5",
        "prettier": "^2.8.4",
