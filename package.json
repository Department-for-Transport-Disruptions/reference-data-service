{
    "name": "reference-data-service",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "aws s3 cp s3://cdd-sandbox-dev-data/.env ./.env.local && sst dev",
        "build": "sst build",
        "deploy": "sst deploy",
        "remove": "sst remove",
        "console": "sst console",
        "diff": "sst diff",
        "sst": "sst",
        "cdk": "cdk",
        "typecheck": "tsc --noEmit",
