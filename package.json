{
    "name": "reference-data-service",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "aws s3 cp s3://cdd-sandbox-dev-data/.env ./.env.local && sst dev",
        "build": "sst build",
        "deploy": "sst deploy",
        "remove": "sst remove",
        "console": "sst console",
        "diff": "sst diff",
        "sst": "sst",
        "cdk": "cdk",
        "typecheck": "tsc --noEmit",
        "lint": "pnpm biome check .",
        "lint:fix": "pnpm biome check --fix --unsafe .",
        "prepare": "husky install",
        "test:uploaders": "pnpm --filter @reference-data-service/ref-data-uploaders test",
        "test:api": "pnpm --filter @reference-data-service/api test",
        "test": "pnpm run test:uploaders -- run && pnpm run test:api -- run",
        "tsc:build": "tsc --noemit"
    },
    "devDependencies": {
        "@aws-sdk/types": "^3.664.0",
        "@biomejs/biome": "1.9.4",
        "@smithy/types": "^3.6.0",
        "@tsconfig/node20": "^20.1.2",
        "@types/aws-lambda": "^8.10.145",
        "@types/lambda-log": "^2.2.1",
        "aws-cdk": "2.161.1",
        "aws-cdk-lib": "2.161.1",
        "aws-sdk-client-mock": "4.1.0",
        "constructs": "10.3.0",
        "husky": "^8.0.0",
        "mockdate": "^3.0.5",
        "sst": "2.44.0",
        "typescript": "^5.3.3",
        "vitest": "^0.29.1"
    },
    "workspaces": ["packages/**/*"],
    "dependencies": {
        "@aws-sdk/client-rds-data": "^3.675.0",
        "@aws-sdk/client-s3": "^3.675.0",
        "@aws-sdk/client-sns": "^3.675.0",
        "@aws-sdk/client-sqs": "^3.675.0",
        "@aws-sdk/client-sts": "^3.675.0",
        "kysely": "^0.27.3",
        "kysely-data-api": "^0.2.0",
        "lambda-log": "^3.1.0"
    },
    "overrides": {
        "jszip": "3.8.0",
        "aws-sdk": "^2.1521.0",
        "xml2js": "0.5.0",
        "nyc": "^15.1.0",
        "mocha": "^10.2.0",
        "chai": "^4.3.7"
    }
}
